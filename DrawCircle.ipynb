    "import turtle\n",
    "\n",
    "pen=turtle.Pen()\n",
    "pen.pensize(5)\n",
    "pen.pencolor(\"green\")\n",
    "pen.fillcolor(\"red\")\n",
    "pen.begin_fill()\n",
    "\n",
    "pen.circle(100)\n",
    "pen.end_fill()\n",
    "turtle.done()"
